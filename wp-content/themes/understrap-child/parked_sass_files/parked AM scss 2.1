    /* Make sure it does not overlap */
    border: none;

    /* Remove borders */
    outline: none;

    /* Remove outline */
    cursor: pointer;

    /* Add a mouse pointer on hover */
    background: $tungsten;
    padding: 1rem .75rem 1rem 0.75rem;
    border-radius: .25rem;
}

.scroll-top svg {
    width: ms(2.5);

    .st0 {
        fill: none;
        stroke: white;
        stroke-width: 5;
    }
}

