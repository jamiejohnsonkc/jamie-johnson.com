// [OVERLAY]
#expt-overlay-1, #expt-overlay-2, #expt-overlay-3, #expt-overlay-4, #expt-overlay-5 {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;

    // height: 120vh;
    background: rgba(0, 0, 0, 0.92);
    z-index: 999;

    // We hide the overlay by default */
    visibility: hidden;
    opacity: 0;
    transition: all 0.5s;
    overflow-y: scroll;

    .expt-body-text, .expt-vert {
        color: $tungsten;
    }
}

#expt-overlay-1 {
    height: 100vh;
    order: 2;

    @include media-breakpoint-up(lg) {
        order: 1;
    }
}

#expt-overlay-2 {
    height: 100vh;
    order: 3;

    @include media-breakpoint-up(lg) {
        order: 2;
    }
}

#expt-overlay-3 {
    height: 100vh;
    order: 4;

    @include media-breakpoint-up(lg) {
        order: 3;
    }
}

#expt-overlay-4 {
    height: 100vh;
    order: 5;

    @include media-breakpoint-up(lg) {
        order: 4;
    }
}

#expt-overlay-5 {
    height: 100vh;
    order: 6;

    @include media-breakpoint-up(lg) {
        order: 5;
    }
}

// This CSS class will show the overlay */
#expt-overlay-1.show, #expt-overlay-2.show, #expt-overlay-3.show, #expt-overlay-4.show, #expt-overlay-5.show {
    visibility: visible;
    opacity: 1;
}

// [OVERLAY CLOSE BUTTON] */
#expt-close-1, #expt-close-2, #expt-close-3, #expt-close-4, #expt-close-5 {
    position: fixed;
    top: $ms-4;
    right: $ms-4;
    color: #fff;
    cursor: pointer;
    z-index: 1000;
    background: $primary;
    height: $ms-3;
    width: $ms-3;
    border-radius: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
}

.exptcontainer {
    display: flex;
    flex-direction: column;
    margin: 0 auto;
}

.expt-intro {
    display: flex;
    align-items: center;
    order: 1;

    @include media-breakpoint-up(md) {
        height: 100vh;
    }


    @include media-breakpoint-up(lg) {
        order: 5;
    }
}

.exptbody-text {
    @include body-4;

    // @include v-1;
    line-height: 1.2;
    max-width: $ms-11;

    @include media-breakpoint-up(md) {}


    @include media-breakpoint-up(lg) {}
}

