        align-items: baseline;
    }

    .webdev-row-head {
        // background: $primary;
        // border-radius: 1em;

        max-width: 70%;

        // margin-bottom: $rvr-1;
        text-align: center;
    }

    .expt-vert {
        @include media-breakpoint-up(lg) {
            @include make-col-ready;
            @include make-col(4);

            height: 315vh;
        }
    }

    .expt-block {
        @include make-col-ready;
        @include make-col(12);

        @include media-breakpoint-up(md) {
            @include make-col-ready;
            @include make-col(6);

            float: right;
        }


        @include media-breakpoint-up(lg) {
            @include make-col-ready;
            @include make-col(6);

            height: 325vh;

            // margin-top: $rvr-3;
            justify-content: center;
            flex-wrap: wrap;
        }
    }

    .expt-block .row + .row {
        display: flex;
        justify-content: center;
        margin-top: $rvr-7;

        @include media-breakpoint-up(lg) {
            max-width: 90%;
        }
    }

    .expt-box {
        @include media-breakpoint-up(sm) {
            @include make-col-ready;
            @include make-col(12);
        }


        @include media-breakpoint-up(md) {
            @include make-col-ready;
            @include make-col(4);

            .expt-text {
                max-width: $ms-10_5;
            }
